{
  "name": "alicard-v2",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "check": "next lint && tsc --noEmit",
    "db:generate": "prisma migrate dev",
    "db:migrate": "node scripts/prisma-with-env.js migrate deploy",
    "db:check": "tsx prisma/check-migration-drift.ts",
    "db:push": "node -e \"console.error('ERROR: db:push is disabled. Use npm run db:generate to create migrations instead.'); process.exit(1)\"",
    "db:release-lock": "node scripts/release-prisma-lock.js",
    "db:force-release-lock": "node scripts/force-release-lock.js",
    "db:verify-schema": "node scripts/verify-schema.js",
    "db:seed": "tsx prisma/seed.ts",
    "db:cleanup": "tsx prisma/pre-migration-cleanup.ts",
    "db:studio": "prisma studio",
    "dev": "next dev --turbo",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "postinstall": "prisma generate",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "preview": "next build && next start",
    "start": "next start",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.7.2",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "@t3-oss/env-nextjs": "^0.13.10",
    "@tanstack/react-query": "^5.69.0",
    "@trpc/client": "^11.8.0",
    "@trpc/react-query": "^11.8.0",
    "@trpc/server": "^11.8.0",
    "bcryptjs": "^3.0.3",
    "next": "^16.1.0",
    "next-auth": "5.0.0-beta.30",
    "pg": "^8.16.3",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-hot-toast": "^2.6.0",
    "server-only": "^0.0.1",
    "superjson": "^2.2.1",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/bcryptjs": "^3.0.0",
    "@types/node": "^25.0.3",
    "@types/pg": "^8.16.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.2",
    "eslint-config-next": "^16.1.0",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "prisma": "^7.2.0",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.19.2",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.50.0"
  },
  "ct3aMetadata": {
    "initVersion": "7.40.0"
  },
  "packageManager": "npm@9.9.4"
}
